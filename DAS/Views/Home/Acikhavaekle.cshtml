@model DAS.Models.MASTER_ACIKHAVA_TURU
@{
    if (Session["account"] == null)
    {
        Response.Redirect("~/Home");
    }
}
<title>Açık Hava Ekle</title>
<h4>Açık Hava Ekle</h4>
<hr />
<div class="row">
    <div class="col-md-5">
        <div class="card card-primary">
            <div class="card-header">
            </div>
            @using (Html.BeginForm())
            {
                <form role="form">
                    <div class="card-body">
                        <div class="form-group">
                            <div class="">
                                <label class="">Açık Hava:</label>
                                @Html.EditorFor(model => model.ACIKHAVA_TURU, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.ACIKHAVA_TURU, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="">
                                <label class="">Açık Hava Master:</label>
                                @Html.EditorFor(model => model.ACIKHAVA_TURU_MASTER, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.ACIKHAVA_TURU_MASTER, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>
                    <div class="card-footer">
                        <button type="submit" class="btn btn-primary">Ekle</button>
                    </div>
                </form>
            }
        </div>
    </div>
</div>